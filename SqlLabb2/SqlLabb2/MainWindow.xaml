<Window x:Class="SqlLabb2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SqlLabb2"
        xmlns:viewmodels="clr-namespace:SqlLabb2.ViewModels"
        xmlns:views="clr-namespace:SqlLabb2.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Resources>
            <DataTemplate DataType="{x:Type viewmodels:EditViewModel}">
                <views:EditView />
            </DataTemplate>
            </Grid.Resources>
    </Grid>
</Window>
